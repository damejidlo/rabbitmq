includes:
	- vendor/phpstan/phpstan-mockery/extension.neon

parameters:
	level: max
	paths:
		- %currentWorkingDirectory%/tests

	fileExtensions:
		- php
		- phpt

	autoload_directories:
		- %currentWorkingDirectory%/tests

	excludes_analyse:
		- %currentWorkingDirectory%/tests/tmp/*

	ignoreErrors:
		- '#Array \(array<PhpAmqpLib\\Channel\\AMQPChannel>\) does not accept int#' # false positive
